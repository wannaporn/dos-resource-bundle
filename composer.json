    "license": "MIT",
    "authors": [
        {
            "name": "ãƒ„ Liverbool",
            "email": "nukboon@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "sylius/resource": "*",
        "sylius/resource-bundle": "*",
        "symfony/yaml": "~2.5",
        "twig/extensions": "^1.2",
        "cocur/slugify": "~1.2",
        "nesbot/carbon": "~1.20"
    },
    "require-dev": {
        "liil/kint": "1.0",
        "phpspec/phpspec": "~2.1"
    },
    "autoload": {
        "psr-4": { "DoS\\ResourceBundle\\": "" }
    },
    "autoload-dev": {
        "psr-4": { "DoS\\ResourceBundle\\spec\\": "spec/" }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
